{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "description": ["Process CrunchyData Postgres version"],
      "fileMatch": [
        "Dockerfile"
      ],
      "matchStrings": [
        "imageName: (?<depName>\\S+):(?<currentValue>-.*\\.*\\-.*)"
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
